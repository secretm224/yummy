#query
type Query {
    hello: String
    helloWithName(name: String): String
    SearchStoreAllData: [SearchStoreDto]
    SearchStoreName(SearchStoreName: String): SearchStoreDto
    SearchStoreList(page: Int!, size: Int!): [SearchStoreDto]
    StoreMajorsTypeList:[StoreTypeMajorDto]
    StoreSubMajorsTypeList(major_code:Int!):[StoreTypeSubDto]
}
#scalar DateTime
type SearchStoreDto {
    address: String
    lat: String
    lng: String
    locationCity: String
    locationCounty: String
    locationDistrict: String
    majorType: [Int]
    name: String
    recommendNames: [String]
    seq: Int
    subType: [Int]
    timestamp: String
    type: String
    zeroPossible: Boolean
}

type StoreTypeMajorDto {
    majorType:Int
    typeName:String
}

type StoreTypeSubDto{
    subType:Int
    majorType:Int
    typeName:String
}

#query

#Mutation
type Mutation {
    addStore(addStoreDto: AddStoreInput!): Boolean!
}

#scalar BigDecimal

input AddStoreInput {
    name: String!
    type: String!
    isBeefulPay: Boolean = false
    lat:Int = 0
    lng:Int = 0
    locationCounty:String!
    locationCity:String!
    locationDistrict:String!
    subType:Int!
    address:String!
}

#Mutation